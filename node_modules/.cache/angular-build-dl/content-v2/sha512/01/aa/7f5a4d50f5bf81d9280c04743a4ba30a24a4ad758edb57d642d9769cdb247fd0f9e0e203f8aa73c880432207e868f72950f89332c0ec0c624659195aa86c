miss().then((data) => {\n      console.log(data);\n      if (data && data.role === 'selected') {\n        console.log('ok');\n        this.mobile.ccCode = '+' + data.data;\n        this.mobileLogin.ccCode = '+' + data.data;\n      }\n    });\n    await modal.present();\n  }\n}\n"],"sourceRoot":"webpack:///"}